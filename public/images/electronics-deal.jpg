<svg xmlns="http://www.w3.org/2000/svg" width="400" height="300" viewBox="0 0 400 300" style="background: linear-gradient(135deg, #11998e 0%, #38ef7d 100%);">
  <rect width="400" height="300" fill="url(#electronicsGradient)"/>
  <defs>
    <linearGradient id="electronicsGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#16a085;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#27ae60;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Laptop Icon -->
  <g transform="translate(200, 150)">
    <rect x="-40" y="-20" width="80" height="30" rx="3" fill="white" opacity="0.9"/>
    <rect x="-35" y="-15" width="70" height="20" fill="#333"/>
    <rect x="-50" y="10" width="100" height="8" rx="4" fill="white" opacity="0.8"/>
    
    <!-- Screen details -->
    <rect x="-30" y="-12" width="60" height="14" fill="#4A90E2"/>
    <circle cx="0" cy="-5" r="1" fill="white"/>
  </g>
  
  <!-- Arabic Text -->
  <text x="200" y="220" text-anchor="middle" fill="white" font-size="22" font-weight="bold" font-family="Arial">
    أجهزة إلكترونية
  </text>
  
  <!-- Decorative elements -->
  <circle cx="60" cy="60" r="15" fill="white" opacity="0.1"/>
  <circle cx="340" cy="240" r="20" fill="white" opacity="0.1"/>
  <circle cx="100" cy="240" r="12" fill="white" opacity="0.1"/>
</svg> 